{
  "expo": {
    "name": "Controle Financeiro",
    "slug": "controle-financeiro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "bundleIdentifier": "com.seuusuario.controlefinanceiro",
      "supportsTablet": true
    },
    "android": {
      "package": "com.n2ilva.controlefinanceiro",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Permitir que o App acesse a c√¢mera para ler QR Codes de Notas Fiscais."
        }
      ],
      "expo-font"
    ],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "38f77e21-2a2b-4fe8-8c49-f0c67c13d767"
      }
    }
  }
}
